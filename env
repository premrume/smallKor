#/bin/bash

# Brute force
DATA_ORIG=./dataOriginal
DATA_OUT=./data

export SRC_TEST=${DATA_ORIG}/kor-test.txt
export SRC_TRAIN=${DATA_ORIG}/kor-train.txt
export TGT_TEST=${DATA_ORIG}/en-test.txt
export TGT_TRAIN=${DATA_ORIG}/en-train.txt

export SRC_TEST_PREP=${DATA_OUT}/src-test.txt
export SRC_TRAIN_PREP=${DATA_OUT}/src-train.txt
export TGT_TEST_PREP=${DATA_OUT}/tgt-test.txt
export TGT_TRAIN_PREP=${DATA_OUT}/tgt-train.txt

# Note src-vocab, tgt-vocab 
export SRC_VOCAB=${DATA_OUT}/src-vocab.txt
export TGT_VOCAB=${DATA_OUT}/tgt-vocab.txt
